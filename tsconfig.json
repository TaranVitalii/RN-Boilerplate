{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "module": "esnext",
    "skipLibCheck": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "resolveJsonModule": true,
    "paths": {
      "@app": [
        "./app"
      ]
    }
  },
  "types": ["jest", "styled-components-react-native"],
  "exclude": [
  "node_modules",
  "babel.config.js",
  "metro.config.js",
  "jest.config.js",
  ".eslintrc.js"
  ]
}
